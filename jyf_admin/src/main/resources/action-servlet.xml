<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd"
       default-autowire="byName">

    <bean id="loginAction" class="cn.mixpay.admin.action.user.LoginAction" scope="prototype"/>
    <bean id="mainAction" class="cn.mixpay.admin.action.user.MainAction" scope="prototype"/>
    <bean id="logoutAction" class="cn.mixpay.admin.action.user.LogoutAction" scope="prototype"/>
    <bean id="userAction" class="cn.mixpay.admin.action.user.UserAction" scope="prototype"/>
    <bean id="roleAction" class="cn.mixpay.admin.action.user.RoleAction" scope="prototype"/>
    <bean id="menuAction" class="cn.mixpay.admin.action.user.MenuAction" scope="prototype"/>
    <bean id="permissionAction" class="cn.mixpay.admin.action.user.PermissionAction" scope="prototype"/>
    <bean id="permissionItemAction" class="cn.mixpay.admin.action.user.PermissionItemAction" scope="prototype"/>
    <bean id="logAction" class="cn.mixpay.admin.action.log.LogAction" scope="prototype"/>

    <bean id="payPlatformConfigAction" class="cn.mixpay.admin.action.business.PayPlatformConfigAction" scope="prototype"/>
    <bean id="merchantAction" class="cn.mixpay.admin.action.merchant.MerchantAction" scope="prototype"/>
    <bean id="merchantAppAction" class="cn.mixpay.admin.action.merchant.MerchantAppAction" scope="prototype"/>
    <bean id="merchantSelectablePayModeAction" class="cn.mixpay.admin.action.merchant.MerchantSelectablePayModeAction" scope="prototype"/>
    <bean id="merchantAppPayModeAction" class="cn.mixpay.admin.action.merchant.MerchantAppPayModeAction" scope="prototype"/>
</beans>